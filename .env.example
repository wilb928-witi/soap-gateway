# ==========================
# DEV profile only
# ==========================
# Use with:
#   export SPRING_PROFILES_ACTIVE=dev
#   cp .env.example .env

# --------------------------
# OAuth2 (DEV)
# --------------------------
DEV_OAUTH2_ENABLED=false
DEV_OAUTH2_CLIENT_ID=backend-client
DEV_OAUTH2_CLIENT_SECRET=secret
DEV_OAUTH2_SCOPE=api.read api.write

# --------------------------
# WS-Security (DEV)
# --------------------------
DEV_WS_SECURITY_ENABLED=false
DEV_WS_SECURITY_USERNAME=
DEV_WS_SECURITY_PASSWORD=

# --------------------------
# Mutual TLS (DEV)
# --------------------------
DEV_MUTUAL_TLS_ENABLED=false
DEV_TLS_KEYSTORE_PATH=
DEV_TLS_KEYSTORE_PASSWORD=
DEV_TLS_TRUSTSTORE_PATH=
DEV_TLS_TRUSTSTORE_PASSWORD=
